Usage:  crlutil -L [-n nickname] [-d keydir] [-P dbprefix] [-t crlType]
        crlutil -D -n nickname [-d keydir] [-P dbprefix]
        crlutil -S -i crl
        crlutil -I -i crl -t crlType [-u url] [-d keydir] [-P dbprefix] [-B] [-p pwd-file] -w [pwd-string]
        crlutil -E -t crlType [-d keydir] [-P dbprefix]
        crlutil -T
